{
    "author": "fodk",
    "description": "Change playback speed to 1.4 on amazon video",
    "name": "AmazonSpeedup",
    "version": "1.1",
    "icons": {
       "40": "icons/icon-48.png"
    },
    "permissions": [
        "*://*/*"
    ],
    
    "content_scripts": [{
        "js": ["content_scripts/cs.js"],
        "matches": ["https://www.amazon.co.jp/gp/video/detail/*"]
    }],
 
    "browser_action": {
       "default_icon": {
         "40": "icons/icon-48.png"
       },
       "default_title": "PopUp",
       "default_popup": "popup/popup.html"
    }
 }