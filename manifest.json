{
    "author": "fodk",
    "description": "Change playback speed to 1.4 on amazon video",
    "name": "AmazonSpeedup",
    "version": "1.0",
    "icons": {
       "20": "icons/icon_20.png",
       "40": "icons/icon_40.png"
    },
 "permissions": [
    "*://*/*"
 ],
 
 "content_scripts": [{
    "js": ["content_scripts/cs.js"],
    "matches": ["*://*/*"]
 }],
 
    "browser_action": {
       "default_icon": {
         "20": "icons/icon_20.png",
         "40": "icons/icon_40.png"
       },
       "default_title": "PopUp",
       "default_popup": "popup/popup.html"
    },
 
    "web_accessible_resources": [
    ]
 }