{
    "author": "FODK",
    "description": "Change playback speed or something with Edge",
    "name": "We-Touch-Speed",
    "version": "1.1",
    "icons": {
       "40": "icons/icon-48.png"
    },
    "permissions": [
        "storage",
        "*://*/*"
    ],
    
    "content_scripts": [{
        "js": ["cs.js"],
        "matches": ["https://www.amazon.co.jp/gp/video/detail/*"]
    }],
 
    "browser_action": {
       "default_icon": {
         "40": "icons/icon-48.png"
       },
       "default_title": "Speed Touch",
       "default_popup": "popup.html"
    },

    "options_page":  "option.html"
 }